<h1>TEST <%= @test.title %> info </h1>

<p>
  <strong>Id:</strong>
  <%= @test.id %>
</p>

<p>
  <strong>Title:</strong>
  <%= @test.title %>
</p>

<p>
  <strong>Level:</strong>
  <%= @test.level %>
</p>

<p>
  <strong>Category:</strong>
  <%= @test.category.title %>
</p>

<p>
  <strong>Created at:</strong>
  <%= @test.created_at %>
</p>

<p>
  <strong>Updated at:</strong>
  <%= @test.updated_at %>
</p>

<p>
  <strong>Questions in TEST <%= @test.title %>:</strong>
</p>

<table>
  <tr>
    <th>Id</th>
    <th>Body</th>
    <th colspan="3"></th>
  </tr>
  <%= render @test.questions %>
</table>

  <%= link_to 'Edit test', edit_test_path(@test.id) %>
  <%= link_to 'Back', tests_path() %>
